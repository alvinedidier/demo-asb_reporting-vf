                        <% if(reporting.globalMetrics.completionRateGlobal != "0.00") { %> <th scope="col"
                            class="text-right">VTR</th>
                        <% } %>
                    </tr>
                </thead>
                <tbody>
                    <% for (let site in reporting.metrics.bySite) { %>
                    <tr>
                        <td><%= site %></td>
                        <td class="text-right"><%= reporting.metrics.bySite[site].impressions.toLocaleString('fr-FR') %>
                        </td>
                        <td class="text-right"><%= reporting.metrics.bySite[site].clics.toLocaleString('fr-FR') %></td>
                        <td class="text-right"><%= reporting.metrics.bySite[site].ctr.toLocaleString('fr-FR') %>%</td>
                        <% if(reporting.globalMetrics.completionRateGlobal != "0.00") { %> <td class="text-right">
