            </table>
        </div>

        <hr />
        <% } %>

        <!-- Affichage par format -->
        <% if (reporting.metrics.byFormat && Object.keys(reporting.metrics.byFormat).length > 0) { %>
        <div class="row p-3 m-5" id="table_global_format">
            <h3 class="title text-asb">
                <svg width="40" height="40" viewBox="0 0 25 24" fill="#112f82" xmlns="http://www.w3.org/2000/svg"
                    transform="rotate(0 0 0) scale(-1, 1) translate(-0, 0)">
                    <path fill-rule="evenodd" clip-rule="evenodd"
                        d="M7.77344 5.25C7.77344 4.00736 8.7808 3 10.0234 3H14.0234C15.2661 3 16.2734 4.00736 16.2734 5.25V6H19.2736C20.5163 6 21.5236 7.00736 21.5236 8.25V17.25C21.5236 18.4926 20.5163 19.5 19.2736 19.5H4.77344C3.5308 19.5 2.52344 18.4926 2.52344 17.25V8.25C2.52344 7.00736 3.5308 6 4.77344 6H7.77344V5.25ZM14.7734 5.25V6H9.27344V5.25C9.27344 4.83579 9.60922 4.5 10.0234 4.5H14.0234C14.4377 4.5 14.7734 4.83579 14.7734 5.25ZM4.77344 7.5H19.2736C19.6879 7.5 20.0236 7.83579 20.0236 8.25V10.5H14.605C14.3242 9.90876 13.7215 9.5 13.0234 9.5H11.0234C10.3253 9.5 9.72271 9.90876 9.44185 10.5H4.02344V8.25C4.02344 7.83579 4.35922 7.5 4.77344 7.5ZM9.44185 12H4.02344V17.25C4.02344 17.6642 4.35922 18 4.77344 18H19.2736C19.6879 18 20.0236 17.6642 20.0236 17.25V12H14.605C14.3242 12.5912 13.7215 13 13.0234 13H11.0234C10.3253 13 9.72271 12.5912 9.44185 12Z"
                        fill="#112f82" />
                </svg>

                <strong>Par Format</strong>
            </h3>
            <table class="table" id="table_campaigns">
                <thead class="text-left table-asb">
                    <tr>
                        <th scope="col">Format</th>
                        <th scope="col" class="text-right">Impressions</th>
                        <th scope="col" class="text-right">Clics</th>
                        <th scope="col" class="text-right">Taux de clics</th>
                        <% if(reporting.globalMetrics.completionRateGlobal != "0.00") { %> <th scope="col"
                            class="text-right">VTR</th>
                        <% } %>
                    </tr>
                </thead>
                <tbody>
                    <% for (let format in reporting.metrics.byFormat) { %>
                    <tr>
                        <td><%= format.toUpperCase() %></td>
                        <td class="text-right">
                            <%= reporting.metrics.byFormat[format].impressions.toLocaleString('fr-FR') %></td>
                        <td class="text-right"><%= reporting.metrics.byFormat[format].clics.toLocaleString('fr-FR') %>
                        </td>
                        <td class="text-right"><%= reporting.metrics.byFormat[format].ctr.toLocaleString('fr-FR') %>%
                        </td>
                        <% if(reporting.globalMetrics.completionRateGlobal != "0.00") { %>
                        <% if(reporting.metrics.byFormat[format].vtr != "0.00") { %> <td class="text-right">
                            <%= reporting.metrics.byFormat[format].vtr.toLocaleString('fr-FR') %>%</td> <% } else { %>
                        <td class="text-right">-</td> <% } %>
                        <% } %>
                    </tr>
                    <% } %>
                </tbody>
                <tfoot class="table-asb">
                    <tr>
                        <td><strong>Total</strong></td>
                        <td class="text-right"><%= reporting.globalMetrics.totalImpressions.toLocaleString('fr-FR') %>
                        </td>
                        <td class="text-right"><%= reporting.globalMetrics.totalClics.toLocaleString('fr-FR') %></td>
                        <td class="text-right"><%= reporting.globalMetrics.ctrGlobal.toLocaleString('fr-FR') %>%</td>
                        <% if(reporting.globalMetrics.completionRateGlobal != "0.00") { %> <td class="text-right">
                            <%= reporting.globalMetrics.completionRateGlobal.toLocaleString('fr-FR') %>%</td> <% } %>
                    </tr>
                </tfoot>
            </table>
        </div>

        <hr />
        <% } %>




        <!-- Affichage par format et par site -->
        <% if (reporting.metrics.byFormatAndSite && Object.keys(reporting.metrics.byFormatAndSite).length > 0) { %>
        <div class="row p-3 m-5" id="table_by_format_site">

            <% for (let format in reporting.metrics.byFormatAndSite) { %>

            <h3 class="title text-asb">
                <svg width="40" height="40" viewBox="0 0 25 24" fill="#112f82" xmlns="http://www.w3.org/2000/svg"
                    transform="rotate(0 0 0)">
