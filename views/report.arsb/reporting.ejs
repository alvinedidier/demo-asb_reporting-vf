                        d="M8.89453 6.93457C7.99707 6.93457 7.26953 7.66211 7.26953 8.55957C7.26953 9.45703 7.99707 10.1846 8.89453 10.1846H8.90453C9.80199 10.1846 10.5295 9.45703 10.5295 8.55957C10.5295 7.66211 9.80199 6.93457 8.90453 6.93457H8.89453Z"
                        fill="#112f82" />
                    <path fill-rule="evenodd" clip-rule="evenodd"
                        d="M3.5 5.5C3.5 4.25736 4.50736 3.25 5.75 3.25H18.75C19.9926 3.25 21 4.25736 21 5.5V18.5C21 19.7426 19.9926 20.75 18.75 20.75H5.75C4.50736 20.75 3.5 19.7426 3.5 18.5V5.5ZM5.75 4.75C5.33579 4.75 5 5.08579 5 5.5V15.5807L6.45103 13.6808C7.35463 12.4977 9.13749 12.5025 10.0347 13.6905L11.2502 15.3001C11.6008 15.7643 12.3234 15.6701 12.5431 15.1315L14.5208 10.2852C15.1881 8.65008 17.3928 8.3859 18.4274 9.81704L19.5 11.3006V5.5C19.5 5.08579 19.1642 4.75 18.75 4.75H5.75ZM5 18.5V18.0031C5.0557 17.962 5.10659 17.9124 5.15073 17.8546L7.64311 14.5913C7.94431 14.1969 8.5386 14.1985 8.83766 14.5945L10.0532 16.2041C11.1048 17.5966 13.2726 17.3139 13.9319 15.6983L15.9096 10.8519C16.1321 10.3069 16.867 10.2188 17.2118 10.6959L19.5 13.8608V18.5C19.5 18.9142 19.1642 19.25 18.75 19.25H5.75C5.33579 19.25 5 18.9142 5 18.5Z"
                        fill="#112f82" />
                </svg>

                <%= format.toUpperCase() %>
            </h3>
            <table class="table table-stripe table-hover md-4">
                <thead class="text-left table-asb">
                    <tr>
                        <th scope="col">Site</th>
                        <th scope="col" class="text-right">Impressions</th>
                        <th scope="col" class="text-right">Clics</th>
                        <th scope="col" class="text-right">Taux de clics</th>
                        <% if (reporting.globalMetrics.completionRateGlobal != "0.00") { %>
                        <th scope="col" class="text-right">VTR</th>
                        <% } %>
                    </tr>
                </thead>
                <tbody>
                    <% 
                            // Trier les sites par ordre alphabétique pour chaque format
                            const sortedSites = Object.keys(reporting.metrics.byFormatAndSite[format]).sort();
                            sortedSites.forEach(site => {
                        %>
                    <tr>
                        <td><%= site %></td>
                        <td class="text-right">
                            <%= reporting.metrics.byFormatAndSite[format][site].impressions.toLocaleString('fr-FR') %>
                        </td>
                        <td class="text-right">
                            <%= reporting.metrics.byFormatAndSite[format][site].clics.toLocaleString('fr-FR') %>
                        </td>
                        <td class="text-right">
                            <%= reporting.metrics.byFormatAndSite[format][site].ctr.toLocaleString('fr-FR') %>%
                        </td>
                        <% if (reporting.globalMetrics.completionRateGlobal != "0.00") { %>
                        <td class="text-right">
                            <%= reporting.metrics.byFormatAndSite[format][site].vtr.toLocaleString('fr-FR') %>%
                        </td>
                        <% } %>
                    </tr>
                    <% }) %>
                </tbody>

            </table>

            <% } %>
        </div>

        <hr />
        <% } %>

        <!-- Affichage par créative -->
        <% if (reporting.metrics.byCreatives && Object.keys(reporting.metrics.byCreatives).length > 0) { %>

        <div class="row p-3 m-5" id="table_by_creative">
            <h3 class="title text-asb">
                <svg width="40" height="40" viewBox="0 0 25 24" fill="#112f82" xmlns="http://www.w3.org/2000/svg"
                    transform="rotate(0 0 0) scale(-1, 1) translate(-0, 0) scale(1, -1) translate(0, -0)">
                    <path
                        d="M8.25016 10.5C7.5598 10.5 7.00016 11.0596 7.00016 11.75C7.00016 12.4404 7.5598 13 8.25016 13H8.26016C8.95051 13 9.51016 12.4404 9.51016 11.75C9.51016 11.0596 8.95051 10.5 8.26016 10.5H8.25016Z"
                        fill="#112f82" />
                    <path fill-rule="evenodd" clip-rule="evenodd"
                        d="M7.75016 5.5C7.75016 4.25736 8.75751 3.25 10.0002 3.25H19.0002C20.2428 3.25 21.2502 4.25736 21.2502 5.5V14.5C21.2502 15.7426 20.2428 16.75 19.0002 16.75H17.2502V18.5C17.2502 19.7426 16.2428 20.75 15.0002 20.75H6.00016C4.75751 20.75 3.75016 19.7426 3.75016 18.5V17.6916C3.74995 17.6814 3.74995 17.6712 3.75016 17.6611V9.5C3.75016 8.25736 4.75751 7.25 6.00016 7.25H7.75016V5.5ZM15.7502 9.5V15.9558L13.4851 13.8525C12.789 13.206 11.7619 13.0665 10.9186 13.5037L5.25016 16.4421L5.25016 9.5C5.25016 9.08579 5.58594 8.75 6.00016 8.75H15.0002C15.4144 8.75 15.7502 9.08579 15.7502 9.5ZM5.25016 18.5V18.1317L11.6089 14.8354C11.89 14.6896 12.2324 14.7362 12.4644 14.9516L15.7502 18.0028V18.5C15.7502 18.9142 15.4144 19.25 15.0002 19.25H6.00016C5.58594 19.25 5.25016 18.9142 5.25016 18.5ZM9.25016 7.25H15.0002C16.2428 7.25 17.2502 8.25736 17.2502 9.5V15.25H19.0002C19.4144 15.25 19.7502 14.9142 19.7502 14.5V5.5C19.7502 5.08579 19.4144 4.75 19.0002 4.75H10.0002C9.58594 4.75 9.25016 5.08579 9.25016 5.5V7.25Z"
                        fill="#112f82" />
                </svg>

                <strong>Par Créative</strong>
            </h3>
            <table class="table table-stripe table-hover">
                <thead class="text-left table-asb">
                    <tr>
                        <th scope="col">Créative</th>
                        <th scope="col" class="text-right">Impressions</th>
                        <th scope="col" class="text-right">Clics</th>
                        <th scope="col" class="text-right">Taux de clics</th>
                    </tr>
                </thead>
                <tbody>
                    <% for (let creative in reporting.metrics.byCreatives) { %>
                    <tr>
                        <td><%= creative %></td>
                        <td class="text-right">
                            <%= reporting.metrics.byCreatives[creative].impressions.toLocaleString('fr-FR') %></td>
                        <td class="text-right">
                            <%= reporting.metrics.byCreatives[creative].clics.toLocaleString('fr-FR') %></td>
                        <td class="text-right">
                            <%= reporting.metrics.byCreatives[creative].ctr.toLocaleString('fr-FR') %>%</td>
                    </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
        <% } %>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous">
    </script>
</body>

</html>